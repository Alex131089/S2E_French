<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory android:title="@string/apps">
        <CheckBoxPreference
                android:title="@string/apps"
                android:defaultValue="false"
                android:key="app"/>
        <CheckBoxPreference
                android:title="@string/private_apps"
                android:defaultValue="false"
                android:key="app-private"/>
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/app_data">
        <CheckBoxPreference
                android:title="@string/app_data"
                android:defaultValue="false"
                android:key="data"/>
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/caches">
        <CheckBoxPreference
                android:title="@string/dalvik_cache"
                android:defaultValue="false"
                android:key="dalvik-cache"/>
    </PreferenceCategory>
</PreferenceScreen>
