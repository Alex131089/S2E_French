<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory android:title="@string/status">
        <Preference android:key="script_status" />
	</PreferenceCategory>
	<PreferenceCategory android:title="@string/setup">
		<CheckBoxPreference
			android:title="@string/apps"
			android:defaultValue="false"
			android:summary="@string/apps__summary"
            android:enabled="false"
			android:key="app" />
		<CheckBoxPreference
			android:title="@string/private_apps"
			android:defaultValue="false"
			android:summary="@string/private_apps__summary"
            android:enabled="false"
			android:key="app-private" />
		<CheckBoxPreference
			android:title="@string/dalvik_cache"
			android:defaultValue="false"
			android:summary="@string/dalvik_cache__summary"
            android:enabled="false"
			android:key="dalvik-cache" />
		<CheckBoxPreference
			android:title="@string/download_cache"
			android:defaultValue="false"
			android:summary="@string/download_cache__summary"
            android:enabled="false"
			android:key="download" />
	</PreferenceCategory>
	<PreferenceCategory android:title="@string/about">
        <Preference
                android:title="@string/app_name_full"
                android:summary="@string/app_by"
                android:enabled="false" />
		<Preference
                android:title="@string/credit"
                android:summary="@string/credit__summary"
                android:enabled="false" />
	</PreferenceCategory>
</PreferenceScreen>
